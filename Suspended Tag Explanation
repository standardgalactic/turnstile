</span><span><span class="hljs-comment"># Create a mind map-style ontology for Aspect Relegation Theory</span></span><span>
mind_map = Digraph(comment=</span><span><span class="hljs-string">'Aspect Relegation Theory - Mind Map'</span></span><span>)

</span><span><span class="hljs-comment"># Root node</span></span><span>
mind_map.node(</span><span><span class="hljs-string">'Root'</span></span><span>, </span><span><span class="hljs-string">'Aspect Relegation Theory'</span></span><span>, shape=</span><span><span class="hljs-string">'ellipse'</span></span><span>, style=</span><span><span class="hljs-string">'filled'</span></span><span>, fillcolor=</span><span><span class="hljs-string">'lightblue'</span></span><span>)

</span><span><span class="hljs-comment"># First layer</span></span><span>
mind_map.node(</span><span><span class="hljs-string">'S2'</span></span><span>, </span><span><span class="hljs-string">'System 2\n(Deliberate Thinking)'</span></span><span>)
mind_map.node(</span><span><span class="hljs-string">'S1'</span></span><span>, </span><span><span class="hljs-string">'System 1\n(Intuition/Automaticity)'</span></span><span>)
mind_map.edge(</span><span><span class="hljs-string">'Root'</span></span><span>, </span><span><span class="hljs-string">'S2'</span></span><span>)
mind_map.edge(</span><span><span class="hljs-string">'Root'</span></span><span>, </span><span><span class="hljs-string">'S1'</span></span><span>)

</span><span><span class="hljs-comment"># Second layer under System 2</span></span><span>
mind_map.node(</span><span><span class="hljs-string">'Repetition'</span></span><span>, </span><span><span class="hljs-string">'Repetition &amp; Practice'</span></span><span>)
mind_map.node(</span><span><span class="hljs-string">'Encoding'</span></span><span>, </span><span><span class="hljs-string">'Multimodal Encoding'</span></span><span>)
mind_map.edge(</span><span><span class="hljs-string">'S2'</span></span><span>, </span><span><span class="hljs-string">'Repetition'</span></span><span>)
mind_map.edge(</span><span><span class="hljs-string">'S2'</span></span><span>, </span><span><span class="hljs-string">'Encoding'</span></span><span>)

