  .motion-safe\:\[transition\:border-color_0\.1s_ease-in-out\] { transition: border-color 0.1s ease-in-out; }
  .motion-safe\:\[transition\:height_0\.3s_var\(--easing-common\)\] { transition: height .3s var(--easing-common); }
