    </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-property">documentElement</span></span><span>.</span><span><span class="hljs-title function_">setAttribute</span></span><span>(</span><span><span class="hljs-string">"data-theme"</span></span><span>, </span><span><span class="hljs-string">"dark"</span></span><span>);
  }

  </span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">toggleTheme</span></span><span>(</span><span><span class="hljs-params"></span></span><span>) {
    </span><span><span class="hljs-keyword">const</span></span><span> current = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-property">documentElement</span></span><span>.</span><span><span class="hljs-title function_">getAttribute</span></span><span>(</span><span><span class="hljs-string">"data-theme"</span></span><span>);
    </span><span><span class="hljs-keyword">if</span></span><span> (current === </span><span><span class="hljs-string">"dark"</span></span><span>) {
      </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-property">documentElement</span></span><span>.</span><span><span class="hljs-title function_">removeAttribute</span></span><span>(</span><span><span class="hljs-string">"data-theme"</span></span><span>);
      </span><span><span class="hljs-variable language_">localStorage</span></span><span>.</span><span><span class="hljs-title function_">setItem</span></span><span>(</span><span><span class="hljs-string">"theme"</span></span><span>, </span><span><span class="hljs-string">"light"</span></span><span>);
    } </span><span><span class="hljs-keyword">else</span></span><span> {
      </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-property">documentElement</span></span><span>.</span><span><span class="hljs-title function_">setAttribute</span></span><span>(</span><span><span class="hljs-string">"data-theme"</span></span><span>, </span><span><span class="hljs-string">"dark"</span></span><span>);
      </span><span><span class="hljs-variable language_">localStorage</span></span><span>.</span><span><span class="hljs-title function_">setItem</span></span><span>(</span><span><span class="hljs-string">"theme"</span></span><span>, </span><span><span class="hljs-string">"dark"</span></span><span>);
    }
  }
</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">script</span></span></span><span>&gt;
</span></span></code></div></div></pre>
<hr data-start="1094" data-end="1097" class="">
<h4 data-start="1099" data-end="1143" class="">2. <strong data-start="1107" data-end="1143">Font Tweaks for Monospace Blocks</strong></h4>
<p data-start="1144" data-end="1240" class="">Right now, <code data-start="1155" data-end="1161">code</code> and <code data-start="1166" data-end="1171">pre</code> use the default <code data-start="1188" data-end="1199">monospace</code> alias. You can specify a better default:</p>
<pre class="overflow-visible!" data-start="1242" data-end="1316"><div class="contain-inline-size rounded-md border-[0.5px] border-token-border-medium relative bg-token-sidebar-surface-primary"><div class="flex items-center text-token-text-secondary px-4 py-2 text-xs font-sans justify-between h-9 bg-token-sidebar-surface-primary dark:bg-token-main-surface-secondary select-none rounded-t-[5px]">css</div><div class="sticky top-9"><div class="absolute right-0 bottom-0 flex h-9 items-center pe-2"><div class="bg-token-sidebar-surface-primary text-token-text-secondary dark:bg-token-main-surface-secondary flex items-center rounded-sm px-2 font-sans text-xs"><span class="" data-state="closed"><button class="flex gap-1 items-center select-none py-1" aria-label="Copiar"><svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" class="icon-sm"><path fill-rule="evenodd" clip-rule="evenodd" d="M7 5C7 3.34315 8.34315 2 10 2H19C20.6569 2 22 3.34315 22 5V14C22 15.6569 20.6569 17 19 17H17V19C17 20.6569 15.6569 22 14 22H5C3.34315 22 2 20.6569 2 19V10C2 8.34315 3.34315 7 5 7H7V5ZM9 7H14C15.6569 7 17 8.34315 17 10V15H19C19.5523 15 20 14.5523 20 14V5C20 4.44772 19.5523 4 19 4H10C9.44772 4 9 4.44772 9 5V7ZM5 9C4.44772 9 4 9.44772 4 10V19C4 19.5523 4.44772 20 5 20H14C14.5523 20 15 19.5523 15 19V10C15 9.44772 14.5523 9 14 9H5Z" fill="currentColor"></path></svg>Copiar cÃ³digo</button></span></div></div></div><div class="overflow-y-auto p-4" dir="ltr"><code class="whitespace-pre! language-css"><span><span><span class="hljs-attribute">font-family</span></span><span>: Consolas, Menlo, Monaco, </span><span><span class="hljs-string">"Courier New"</span></span><span>, monospace;
</span></span></code></div></div></pre>
<hr data-start="1318" data-end="1321" class="">
<h4 data-start="1323" data-end="1367" class="">3. <strong data-start="1331" data-end="1367">Add a Favicon (Optional but Fun)</strong></h4>
<p data-start="1368" data-end="1406" class="">Add inside <code data-start="1379" data-end="1387">&lt;head&gt;</code> if youâ€™ve got one:</p>
<pre class="overflow-visible!" data-start="1408" data-end="1477"><div class="contain-inline-size rounded-md border-[0.5px] border-token-border-medium relative bg-token-sidebar-surface-primary"><div class="flex items-center text-token-text-secondary px-4 py-2 text-xs font-sans justify-between h-9 bg-token-sidebar-surface-primary dark:bg-token-main-surface-secondary select-none rounded-t-[5px]">html</div><div class="sticky top-9"><div class="absolute right-0 bottom-0 flex h-9 items-center pe-2"><div class="bg-token-sidebar-surface-primary text-token-text-secondary dark:bg-token-main-surface-secondary flex items-center rounded-sm px-2 font-sans text-xs"><span class="" data-state="closed"><button class="flex gap-1 items-center select-none py-1" aria-label="Copiar"><svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" class="icon-sm"><path fill-rule="evenodd" clip-rule="evenodd" d="M7 5C7 3.34315 8.34315 2 10 2H19C20.6569 2 22 3.34315 22 5V14C22 15.6569 20.6569 17 19 17H17V19C17 20.6569 15.6569 22 14 22H5C3.34315 22 2 20.6569 2 19V10C2 8.34315 3.34315 7 5 7H7V5ZM9 7H14C15.6569 7 17 8.34315 17 10V15H19C19.5523 15 20 14.5523 20 14V5C20 4.44772 19.5523 4 19 4H10C9.44772 4 9 4.44772 9 5V7ZM5 9C4.44772 9 4 9.44772 4 10V19C4 19.5523 4.44772 20 5 20H14C14.5523 20 15 19.5523 15 19V10C15 9.44772 14.5523 9 14 9H5Z" fill="currentColor"></path></svg>Copiar cÃ³digo</button></span></div></div></div><div class="overflow-y-auto p-4" dir="ltr"><code class="whitespace-pre! language-html"><span><span><span class="hljs-tag">&lt;<span class="hljs-name">link</span></span></span><span> </span><span><span class="hljs-attr">rel</span></span><span>=</span><span><span class="hljs-string">"icon"</span></span><span> </span><span><span class="hljs-attr">href</span></span><span>=</span><span><span class="hljs-string">"/favicon.ico"</span></span><span> </span><span><span class="hljs-attr">type</span></span><span>=</span><span><span class="hljs-string">"image/x-icon"</span></span><span>&gt;
</span></span></code></div></div></pre>
<p data-start="1479" data-end="1584" class="">You can toss any <code data-start="1496" data-end="1502">.ico</code> file in your <code data-start="1516" data-end="1525">public/</code> folder and itâ€™ll just work with your Crystal server setup.</p>
<hr data-start="1586" data-end="1589" class="">
<h4 data-start="1591" data-end="1637" class="">4. <strong data-start="1599" data-end="1637">Dark Mode Toggle Icon (Bonus Cool)</strong></h4>
<p data-start="1638" data-end="1727" class="">If you want visual feedback (sun/moon emoji for example), you can tweak the button label:</p>
<pre class="overflow-visible!" data-start="1729" data-end="1810"><div class="contain-inline-size rounded-md border-[0.5px] border-token-border-medium relative bg-token-sidebar-surface-primary"><div class="flex items-center text-token-text-secondary px-4 py-2 text-xs font-sans justify-between h-9 bg-token-sidebar-surface-primary dark:bg-token-main-surface-secondary select-none rounded-t-[5px]">html</div><div class="sticky top-9"><div class="absolute right-0 bottom-0 flex h-9 items-center pe-2"><div class="bg-token-sidebar-surface-primary text-token-text-secondary dark:bg-token-main-surface-secondary flex items-center rounded-sm px-2 font-sans text-xs"><span class="" data-state="closed"><button class="flex gap-1 items-center select-none py-1" aria-label="Copiar"><svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" class="icon-sm"><path fill-rule="evenodd" clip-rule="evenodd" d="M7 5C7 3.34315 8.34315 2 10 2H19C20.6569 2 22 3.34315 22 5V14C22 15.6569 20.6569 17 19 17H17V19C17 20.6569 15.6569 22 14 22H5C3.34315 22 2 20.6569 2 19V10C2 8.34315 3.34315 7 5 7H7V5ZM9 7H14C15.6569 7 17 8.34315 17 10V15H19C19.5523 15 20 14.5523 20 14V5C20 4.44772 19.5523 4 19 4H10C9.44772 4 9 4.44772 9 5V7ZM5 9C4.44772 9 4 9.44772 4 10V19C4 19.5523 4.44772 20 5 20H14C14.5523 20 15 19.5523 15 19V10C15 9.44772 14.5523 9 14 9H5Z" fill="currentColor"></path></svg>Copiar cÃ³digo</button></span></div></div></div><div class="overflow-y-auto p-4" dir="ltr"><code class="whitespace-pre! language-html"><span><span><span class="hljs-tag">&lt;<span class="hljs-name">button</span></span></span><span> </span><span><span class="hljs-attr">class</span></span><span>=</span><span><span class="hljs-string">"theme-toggle"</span></span><span> </span><span><span class="hljs-attr">onclick</span></span><span>=</span><span><span class="hljs-string">"toggleTheme()"</span></span><span>&gt;ðŸŒž / ðŸŒ™</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">button</span></s