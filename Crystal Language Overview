  &lt;h1&gt;Crystal Language Basics&lt;/h1&gt;

  &lt;section&gt;
    &lt;h2&gt;Variables &amp; Types&lt;/h2&gt;
    &lt;pre&gt;&lt;code&gt;# Type is inferred
name = "Crystal"
version = 1.10

# Explicit typing
count : Int32 = 42
&lt;/code&gt;&lt;/pre&gt;
  &lt;/section&gt;

  &lt;section&gt;
    &lt;h2&gt;Control Flow&lt;/h2&gt;
    &lt;pre&gt;&lt;code&gt;if count &gt; 10
  puts "High count"
elsif count == 10
  puts "Just right"
else
  puts "Low count"
end
&lt;/code&gt;&lt;/pre&gt;
  &lt;/section&gt;

  &lt;section&gt;
    &lt;h2&gt;Methods&lt;/h2&gt;
    &lt;pre&gt;&lt;code&gt;def greet(name : String) : String
  "Hello, #{name}!"
end

puts greet("Crystal")
&lt;/code&gt;&lt;/pre&gt;
  &lt;/section&gt;

  &lt;section&gt;
    &lt;h2&gt;Classes &amp; Structs&lt;/h2&gt;
    &lt;pre&gt;&lt;code&gt;class Dog
  property name

  def initialize(@name : String)
  end

  def speak
    "Woof! Iâ€™m #{@name}"
  end
end

fido = Dog.new("Fido")
puts fido.speak
&lt;/code&gt;&lt;/pre&gt;
  &lt;/section&gt;

  &lt;section&gt;
    &lt;h2&gt;Collections&lt;/h2&gt;
